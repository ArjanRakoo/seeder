# Environment variables for the database seeder
# Copy this file to .env and customize for your environment

# Backend API base URL
API_BASE_URL=http://localhost:3000/api

# Admin credentials for authentication
ADMIN_USERNAME=admin
ADMIN_PASSWORD=admin123

# Authentication context and platform
AUTH_CONTEXT=admin
AUTH_PLATFORM=web

# Request timeout in milliseconds
REQUEST_TIMEOUT=30000

# Enable verbose logging (true/false)
VERBOSE=false

# SSL certificate validation (set to false for local development with self-signed certificates)
# WARNING: Only use false in development environments!
REJECT_UNAUTHORIZED=true
